<?xml version="1.0"?> 
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
    <hibernate-mapping>
      
    <class name="com.rms.hibernate.accesscontrolbean"
    table="accesscontrol">
    
    <id name="pkid" type="java.lang.Long" column="pk_id">
			<generator class="increment" />
		</id>
					
    <property 
		name="username" 
		type="java.lang.String" 
		column="username"
		
		length="255" />
	
	 <property 
		name="pass" 
		type="java.lang.String" 
		column="password"
		
		length="255" />
		
		 <property 
		name="usertype" 
		type="java.lang.String" 
		column="usertype"
		
		length="255" />		
		
		<property 
		name="hotelname" 
		type="java.lang.String" 
		column="hotelname"
		
		length="255" />		
	
		<property 
		name="hotelid" 
		type="java.lang.Long" 
		column="hotelid"	
		length="25" />

		<property 
		name="userid" 
		type="java.lang.Long" 
		column="userid"
		length="25" />
		
    </class>
   </hibernate-mapping> 