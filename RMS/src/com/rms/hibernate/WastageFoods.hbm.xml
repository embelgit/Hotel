<?xml version="1.0"?> 
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
    <hibernate-mapping>
      
    <class name="com.rms.hibernate.WastageFoods"
    table="wastage_food_entry"
    >
    
    <id name="pk_wastage_food_id" 
        type="java.lang.Long" 
        column="pk_wastage_food_id">
			
			<generator class="increment" />
		    </id>
     
      <property
		name="fk_item_id" 
		type="java.lang.Long" 
		column="fk_item_name_id"
		not-null="true"
        length="19"
        insert="false"
        update="false"
		/>
		
		<property 
		name="foodAmount" 
		type="java.lang.Double" 
		column="amount"
		length="255" />
		
    </class>
   </hibernate-mapping> 