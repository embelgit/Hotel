<?xml version="1.0"?> 
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
    <hibernate-mapping>
      
    <class name="com.rms.hibernate.usercreateBean"
    table="user_details"
    >
    
    <id name="pk_create_user_id" type="java.lang.Long" column="pk_create_user_id">
			<generator class="increment" />
		</id>
     
    <property 
		name="userType" 
		type="java.lang.String" 
		column="user_type"
		
		length="255" />
		
		<property 
		name="firstName" 
		type="java.lang.String" 
		column="first_name"
		 
		length="255" />
		
		<property 
		name="lastName" 
		type="java.lang.String" 
		column="last_name"
		 
		length="255" />
		
		<property 
		name="userName" 
		type="java.lang.String" 
		column="user_name"
		 />
		 
		 <property 
		name="mobileNO" 
		type="java.lang.String" 
		column="mobile_no"
		 />
		 
		<property 
		name="password" 
		type="java.lang.String" 
		column="password"
		 
		length="20" />
		
		<property 
		name="confirmPassword" 
		type="java.lang.String" 
		column="confirm_password"
		 
		length="255" />
		
		<property 
		name="emailId" 
		type="java.lang.String" 
		column="email_id"
		 
		length="255" />
		
		<property 
		name="isActiveYN" 
		type="java.lang.String" 
		column="is_active_YN"
		 
		length="255" />
		
		<property 
		name="insertDate" 
		type="java.util.Date" 
		column="insert_date"
		 
		length="255" />
		
		<property 
		name="modifiedDate" 
		type="java.util.Date" 
		column="modified_date"
		 
		length="255" />
		
		<property 
		name="hotelname" 
		type="java.lang.String" 
		column="hotelName"
		 />
		
		<property 
		name="hotelid" 
		type="java.lang.Long" 
		column="hotelid"
		 />
		 
    </class>
   </hibernate-mapping> 