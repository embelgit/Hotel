<?xml version="1.0"?> 
 <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
    <hibernate-mapping>
    <class name="com.rms.hibernate.CustomerOrder"
    table="customerOrder"
    >
    
    <id name="pk_custOrder_id" type="java.lang.Long" column="pk_custOrder_id">
			<generator class="increment" />
		</id>
     
    <property
		name="tableNo" 
		type="java.lang.Long" 
		column="TableNo"
		/>
		
		
		<property
		name="itemName" 
		type="java.lang.String" 
		column="Itemname"
		length="255" />
		
		<property
		name="waiterName" 
		type="java.lang.String" 
		column="WaiterName"
		length="255" />
		
		
		<property
		name="totalAmount" 
		type="java.lang.Double" 
		column="totalAmount"
		 />
		
		
		 <property
		name="quantity" 
		type="java.lang.Double" 
		column="Quantity"
		 />
		
		 <property
		name="unitPrice" 
		type="java.lang.Double" 
		column="unitPrice"
		length="255" />
		
		 <property
		name="unit" 
		type="java.lang.String" 
		column="unit"
		length="255" />
		
	 <property
		name="value" 
		type="java.lang.Long" 
		column="value"
		length="255" />
		
		<property
		name="GridTotal" 
		type="java.lang.String" 
		column="GridTotal"
		 />
		
	<!-- 	 <property
		name="totalAmt" 
		type="java.lang.Double" 
		column="ToatlAmt"
		length="255" />
		 -->
		<property
		name="billNo" 
		type="java.lang.Long" 
		column="BillNo"
		/>
		
		
		 <property
		name="current_date" 
		type="java.sql.Date" 
		column="CurrentDate"
		length="255" />
		
	 	<property
		name="gst" 
		type="java.lang.Double" 
		column="GST"
		length="20" />
		
		<property
		name="gstAmt" 
		type="java.lang.Double" 
		column="GSTAmount"
		length="20" />
		
		<property
		name="cgst" 
		type="java.lang.Double" 
		column="CGST"
		length="25" />
		
		<property
		name="sgst" 
		type="java.lang.Double" 
		column="SGST"
		length="25" />
		
		<property
		name="type" 
		type="java.lang.String" 
		column="type"
		 />
		 <property
		name="orderbill" 
		type="java.lang.String" 
		column="orderBill"
		 />
		 
		 <property
		name="username" 
		type="java.lang.String" 
		column="username"
		length="255" />
		
		<property
		name="hotelname" 
		type="java.lang.String" 
		column="hotelname"
		length="255"/>
		
		<property
		name="userid" 
		type="java.lang.Long" 
		column="userid"
		length="25"/>
		
		<property
		name="hotelid" 
		type="java.lang.Long" 
		column="hotelid"
		length="25"/>
		
    </class>
   </hibernate-mapping> 