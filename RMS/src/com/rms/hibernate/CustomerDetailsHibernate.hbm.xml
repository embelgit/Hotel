    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 

    <hibernate-mapping>
    
    <class name="com.rms.hibernate.CustomerDetailsHibernate" 
    table="customerdetails"
    >
    
    <id name="pk_cust_id" 
    	type="java.lang.Long" 
    	column="pk_cust_id">
		<generator class="increment" />
		</id>

		<property 
		name="customerName" 
		type="java.lang.String" 
		column="cutomer_name"
		length="255"
		/>

		<property 
		name="dob" 
		type="java.util.Date"  
		column="dob"
		length="255"
		/>
		
		<property 
		name="mobileNumber" 
		type="java.lang.Long" 
		column="mob_no"
		length="15"
		/>

    
