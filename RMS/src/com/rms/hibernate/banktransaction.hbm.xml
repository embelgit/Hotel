<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 

    <hibernate-mapping>
    
    <class name="com.rms.hibernate.banktransaction" table="bank_transaction"
    >
    
    	<id name="pkbankid" type="java.lang.Long" column="pkbank_id">
			<generator class="increment" />
		</id>
		
		
	
		
		<property 
		name="contactno" 
		type="java.lang.Double" 
		column="contactno"
		not-null="true" 
		length="255" />
		
		<property
        name="datee"
        type="java.sql.Date"
        column="datee"
        not-null="false"        
        />
		
		<property 
		name="bankname" 
		type="java.lang.String" 
		column="bankname"
		not-null="true" 
		length="255" />
		
		<property 
		name="paymentType" 
		type="java.lang.String" 
		column="paymentType"
		not-null="true" 
		length="255" />
		    
        
		<property
        name="amount"
        type="java.lang.Double"
        column="amount"
        length="50"
        />
        
		 <property
        name="accountname"
        type="java.lang.String"
        column="accountname"
        length="50"
        />
                
        
          <property
        name="transID"
        type="java.lang.Long"
        column="transactionID"
        /> 
        
        <property 
		name="username" 
		type="java.lang.String" 
		column="username"
		length="255"/>
		
		<property 
		name="hotelname" 
		type="java.lang.String" 
		column="hotelname"
		length="255"/>
		
		<property 
		name="userid" 
		type="java.lang.Long" 
		column="userid"
		length="25"/>
		
		<property 
		name="hotelid" 
		type="java.lang.Long" 
		column="hotelid"
		length="25"/>

		</class>

    </hibernate-mapping>