		length="255" />
		
	 <property 
		name="price" 
		type="java.lang.String" 
		column="price"
		
		length="255" />	 -->	
		
	<property  
		name="total" 
		type="java.lang.String" 
		column="total"
		
		length="255" />		

    </class>
