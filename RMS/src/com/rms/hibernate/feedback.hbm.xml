<?xml version="1.0"?> 
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

	<class name="com.rms.hibernate.feedback" table="feedback">

		<id name="id" type="java.lang.Long" column="pk_feedback_id"> <generator class="increment" /> </id>

		<property name="name" type="java.lang.String" column="name"	length="255" />

		<property name="contact" type="java.lang.Long" column="contact"	length="255" />

		<property name="tfeedback" type="java.lang.String" column="feedback" length="255" />

		<property name="Rate" type="java.lang.String" column="Rate"	length="255" />

		<property name="iDate" type="java.util.Date" column="is_insert_date" length="255" />
		
	</class>
	
</hibernate-mapping> 