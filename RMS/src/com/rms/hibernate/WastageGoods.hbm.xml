<?xml version="1.0"?> 
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" > 
    <hibernate-mapping>
      
    <class name="com.rms.hibernate.WastageGoods"
    table="wastage_good_entry"
    >
    
    <id name="pk_wastage_good_id" type="java.lang.Long" column="pk_wastage_good_id">
			<generator class="increment" />
		</id>
     
    <property 
		name="vagetablesName" 
		type="java.lang.String" 
		column="vegetables_name"
		
		length="255" />
		
		<property 
		name="amount" 
		type="java.lang.Double" 
		column="amount"
		 
		length="255" />

    </class>
   </hibernate-mapping> 